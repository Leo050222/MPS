    },
    {
      "expr": "x^n + (2+x)^n + (2-x)^n = 0",
      "id": "A1577_C2",
      "type": "context"
    },
    {
      "expr": "if n even then x^n ≥ 0, (2+x)^n ≥ 0, (2-x)^n ≥ 0 for all real x",
      "id": "A1577_C3",
      "type": "derived"
    },
    {
      "expr": "if n even then x^n + (2+x)^n + (2-x)^n > 0 for all real x",
      "id": "A1577_C4",
      "type": "derived"
    },
    {
      "expr": "if n even then no real solutions, hence no integer solutions",
      "id": "A1577_C5",
      "type": "derived"
    },
    {
      "expr": "n must be odd",
      "id": "A1577_C6",
      "type": "derived"
    },
    {
      "expr": "for n=1, x^1 + (2+x)^1 + (2-x)^1 = x + 2 + x + 2 - x = x + 4",
      "id": "A1577_C7",
      "type": "derived"
    },
    {
      "expr": "for n=1, x + 4 = 0 ⇒ x = -4 ∈ ℤ",
      "id": "A1577_C8",
      "type": "derived"
    },
    {
      "expr": "for n=1, there exists integer solution x=-4",
      "id": "A1577_C9",
      "type": "derived"
    },
    {
      "expr": "assume odd n > 1 and ∃ integer x with x^n + (2+x)^n + (2-x)^n = 0",
      "id": "A1577_C10",
      "type": "derived"
    },
    {
      "expr": "x, 2+x, 2-x have same parity as x",
      "id": "A1577_C11",
      "type": "derived"
    },
    {
      "expr": "if x odd then x^n, (2+x)^n, (2-x)^n all odd (n odd), sum odd ≠ 0",
      "id": "A1577_C12",
      "type": "derived"
    },
    {
      "expr": "thus x even",
      "id": "A1577_C13",
      "type": "derived"
    },
    {
      "expr": "let x = 2y with y ∈ ℤ",
      "id": "A1577_C14",
      "type": "derived"
    },
    {
      "expr": "(2y)^n + (2+2y)^n + (2-2y)^n = 2^n y^n + 2^n (1+y)^n + 2^n (1-y)^n = 2^n (y^n + (1+y)^n + (1-y)^n) = 0",
      "id": "A1577_C15",
      "type": "derived"
    },
    {
      "expr": "y^n + (1+y)^n + (1-y)^n = 0",
      "id": "A1577_C16",
      "type": "derived"
    },
    {
      "expr": "n odd ⇒ y^n + (1+y)^n + (1-y)^n ≡ y + (1+y) + (1-y) (mod 2)",
      "id": "A1577_C17",
      "type": "derived"
    },
    {
      "expr": "y + (1+y) + (1-y) = y + 1 + y + 1 - y = y + 2 ≡ y (mod 2)",
      "id": "A1577_C18",
      "type": "derived"
    },
    {
      "expr": "y ≡ 0 (mod 2), so y even",
      "id": "A1577_C19",
      "type": "derived"
    },
    {
      "expr": "n odd ⇒ (1+y)^n + (1-y)^n = ((1+y) + (1-y)) \\sum_{k=0}^{n-1} (-1)^k (1+y)^{n-1-k} (1-y)^k = 2 P",
      "id": "A1577_C20",
      "type": "derived"
    },
    {
      "expr": "P = \\sum_{k=0}^{n-1} (-1)^k (1+y)^{n-1-k} (1-y)^k",
      "id": "A1577_C21",
      "type": "derived"
    },
    {
      "expr": "y even ⇒ 1+y odd, 1-y odd",
      "id": "A1577_C22",
      "type": "derived"
    },
    {
      "expr": "(1+y)^{n-1-k} (1-y)^k odd for each k",
      "id": "A1577_C23",
      "type": "derived"
    },
    {
      "expr": "(-1)^k (1+y)^{n-1-k} (1-y)^k ≡ odd (mod 2) for each k",
      "id": "A1577_C24",
      "type": "derived"
    },
    {
      "expr": "P ≡ sum of n odds ≡ n ≡ 1 (mod 2), so P odd",
      "id": "A1577_C25",
      "type": "derived"
    },
    {
      "expr": "y^n + 2 P = 0 ⇒ y^n = -2 P",
      "id": "A1577_C26",
      "type": "derived"
    },
    {
      "expr": "v_2(y^n) = n v_2(y) ≥ n \\cdot 1 ≥ 3 (since y even, n ≥ 3 odd)",
      "id": "A1577_C27",
      "type": "derived"
    },
    {
      "expr": "v_2(-2 P) = v_2(2) + v_2(P) = 1 + 0 = 1",
      "id": "A1577_C28",
      "type": "derived"
    },
    {
      "expr": "n v_2(y) ≥ 3 > 1 = v_2(-2 P), contradiction",
      "id": "A1577_C29",
      "type": "derived"
    },
    {
      "expr": "no integer x for odd n > 1",
      "id": "A1577_C30",
      "type": "derived"
    },
    {
      "expr": "only n=1",
      "id": "A1577_C31",
      "type": "final"
